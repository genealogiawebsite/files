{
    "name": "genealogiawebsite/files",
    "description": "File manager dependency for Laravel Enso",
    "keywords": [
        "genealogiawebsite",
        "files",
        "file-manager",
        "file-uploader"
    ],
    "homepage": "https://github.com/genealogiawebsite/files",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Adrian Ocneanu",
        "email": "aocneanu@gmail.com",
        "homepage": "https://genealogiawebsite.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.4.0",
        "genealogiawebsite/core": "^5.0|^.6.0",
        "genealogiawebsite/helpers": "^2.0",
        "genealogiawebsite/image-transformer": "^2.0",
        "genealogiawebsite/migrator": "^2.0",
        "genealogiawebsite/track-who": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Files\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Files\\AppServiceProvider",
                "LaravelEnso\\Files\\AuthServiceProvider",
                "LaravelEnso\\Files\\FileServiceProvider",
                "LaravelEnso\\Files\\UploadServiceProvider"
            ],
            "aliases": {
                "FileBrowser": "LaravelEnso\\Files\\Facades\\FileBrowser"
            }
        }
    }
}
